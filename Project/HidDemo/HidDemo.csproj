<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
      <OutputType>WinExe</OutputType>
      <TargetFramework>net461</TargetFramework>
      <UseWindowsForms>true</UseWindowsForms>
      <Authors>Stéphane Lenclud</Authors>
      <Description>HID Demo</Description>
      <Copyright>(c)2021 Stéphane Lenclud</Copyright>
      <RootNamespace>HidDemo</RootNamespace>
      <AssemblyName>HidDemo</AssemblyName>
      <PackageId>HidDemo</PackageId>
      <Company>Slions</Company>
      <GenerateResourceUsePreserializedResources>true</GenerateResourceUsePreserializedResources>
      <Platforms>AnyCPU;x64;x86</Platforms>
	  <AllowUnsafeBlocks>True</AllowUnsafeBlocks>  
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="squirrel.windows" Version="2.0.1" />
    <PackageReference Include="System.Resources.Extensions" Version="5.0.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Hid\Hid.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="System.Configuration" />
  </ItemGroup>
</Project>